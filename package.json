{
  "name": "TapasMarketing",
  "version": "0.1.0",
  "description": "For marketing part.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "./node_modules/.bin/webpack-dev-server --config webpack.config.dev.js --history-api-fallback --hot --inline --progress --colors",
    "build": "sh ./bin/build.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TapasTech/TapasMarketing.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TapasTech/TapasMarketing/issues"
  },
  "homepage": "https://github.com/TapasTech/TapasMarketing#readme",
  "config": {
    "ghooks": {
      "commit-msg": "node ./node_modules/.bin/validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    }
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.6.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "ghooks": "^1.0.3",
    "html-webpack-plugin": "^2.9.0",
    "lodash": "^4.1.0",
    "open-browser-webpack-plugin": "0.0.2",
    "postcss-loader": "^0.8.1",
    "qiniu": "^6.1.9",
    "raw-loader": "^0.5.1",
    "rucksack-css": "^0.8.5",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "validate-commit-msg": "^1.1.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
